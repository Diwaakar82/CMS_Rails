
<h1>Posts</h1>
 
<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Likes</th>
    <th></th>
    <th></th>
  </tr>
 
  <% @posts.each do |post| %>
    <tr>
      <td><%= post.title %></td>
      <td><%= post.description %></td>
      <td><%= post.likes %></td>
      <td><%= link_to 'Show', post_path(post) %></td>
      <td type="button" name="Like" onclick="<% post.likes %>">Like</td>
    </tr>
  <% end %>
</table>