<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @category.title %>
</p>

<table>
  <tr>
    <th>Post</th>
    <th></th>
  </tr>

  <% @category.posts.each do |post| %>
    <tr>
      <td><%= post.title %></td>
      <td><%= link_to 'Show', post_path(post) %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'Back', categories_path %>
